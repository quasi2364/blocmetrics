
<%= pie_chart @application.events.group(:name).count %></br>
<%= line_chart @application.events.group_by_day(:created_at).count %>

<table class="table table-hover">
	<thead>
		<tr>
			<th>Event Name</th>
			<th>Count</th>
		</tr>
	</thead>
	<tbody>
		<% @events.each do |key, value| %>
			<tr>
				<td><%= key %></td>
				<td><%= value.count %></td>
			</tr>
		<% end %>
	</tbody>
</table>


